<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editCaption"
        class="com.company.accumed.web.ministryofhealthoffice.MinistryOfHealthOfficeEdit"
        datasource="ministryOfHealthOfficeDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.accumed.web.ministryofhealthoffice">
    <dsContext>
        <datasource id="ministryOfHealthOfficeDs"
                    class="com.company.accumed.entity.MinistryOfHealthOffice"
                    view="_local"/>
    </dsContext>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="ministryOfHealthOfficeDs">
            <column width="250px">
                <field id="officeCode"/>
                <field id="officeCity"/>
                <field id="claimCardCode"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
