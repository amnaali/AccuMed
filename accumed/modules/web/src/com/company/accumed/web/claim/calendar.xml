<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.company.accumed.web.claim.Calendar"
        messagesPack="com.company.accumed.web.claim">
    <dialogMode closeable="true"
                forceDialog="true"
                modal="true"
                width="255"/>
    <layout>
        <fieldGroup id="fieldGroup"
                    width="100%">
            <field id="dates"
                   custom="true"
                   width="100%"/>
        </fieldGroup>
        <buttonsPanel>
            <button id="okbutton"
                    caption="OK"
                    invoke="okaction"/>
            <button id="cancelbutton"
                    caption="Cancel"
                    invoke="cancelaction"/>
        </buttonsPanel>
    </layout>
</window>
