<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editCaption"
        class="com.company.accumed.web.speciality.SpecialityEdit"
        datasource="specialityDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.accumed.web.speciality">
    <dsContext>
        <datasource id="specialityDs"
                    class="com.company.accumed.entity.Speciality"
                    view="_local"/>
    </dsContext>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="specialityDs">
            <column width="250px">
                <field id="specialtyCode"/>
                <field id="specialtyName"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
